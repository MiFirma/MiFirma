<% headers = ["ID", "Nombre", "Apellidos", "DNI", "Email", "Telefono", "Pliegos", "Fecha_creacion"] %>
<%= CSV.generate_line headers %>
<% @attestors.each do |attestor|%>
<% row = [ attestor.id,
				%Q["#{attestor.name}"], 
				%Q["#{attestor.surname}"], 
				attestor.dni, 
				attestor.email,
				attestor.telephone,
				attestor.number_of_sheets,
				attestor.created_at] %>
<%= (CSV.generate_line row , {:row_sep => nil}).html_safe %>
<% end %>